{
  "name": "cams",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/__tests__/helpers/*.js?(x)"
    ]
  },
  "dependencies": {
    "axios": "^0.15.3",
    "axios-mock-adapter": "^1.9.0",
    "enzyme": "^2.8.1",
    "enzyme-to-json": "^2.0.1",
    "font-awesome": "^4.7.0",
    "get-youtube-id": "^1.0.0",
    "localstorage-mock": "^1.0.2",
    "prettier": "^1.7.1",
    "prop-types": "^15.5.10",
    "radium": "^0.18.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-file-input": "^0.2.5",
    "react-motion": "^0.4.7",
    "react-router": "^4.0.0-beta.8",
    "react-router-dom": "^4.0.0",
    "react-simple-dropdown": "^2.0.0",
    "react-sortable-hoc": "^0.6.1",
    "react-test-renderer": "^15.6.2",
    "react-youtube": "^7.3.0",
    "recordrtc": "^5.4.1",
    "videojs-record": "^1.6.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5",
    "video.js": "^5.18.4"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
